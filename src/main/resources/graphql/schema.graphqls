
type Person{
    id : ID!
    name : String
    age : Int
}

type Query{
    persons : [Person]
    personById(id : ID!): Person
}

type Mutation{
    savePerson(name : String! ,age : Int!): Person
    updatePerson(id : ID! ,name : String! ,age : Int!) :Person
    deletePersonById(id : ID!):String
}